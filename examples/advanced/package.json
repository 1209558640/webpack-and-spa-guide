    "autoprefixer": "8.2.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-preset-env": "1.6.1",
    "babel-preset-stage-2": "6.24.1",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-enough": "0.3.0",
    "eslint-loader": "2.0.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.0.7",
    "postcss-loader": "2.1.3",
    "style-loader": "0.20.3",
    "url-loader": "1.0.1",
